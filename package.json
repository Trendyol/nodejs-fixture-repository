{
  "name": "fixture-repository",
  "version": "1.1.2",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "ismail hakki tekin <ihakkitekin@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ihakkitekin/fixture-repository"
  },
  "homepage": "https://github.com/ihakkitekin/fixture-repository",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "fake": "ts-node src/faker/index.ts",
    "test:watch": "jest --watchAll",
    "build": "tsc",
    "prepublish": "yarn cov && yarn build",
    "cov": "jest --coverage",
    "format": "prettier --write --loglevel=error \"src/**/*\" \"__test__/**/*\"",
    "lint": "tslint -c tslint.json --project tsconfig.json \"src/**/*\" \"__test__/**/*\""
  },
  "dependencies": {
    "faker": "^4.1.0",
    "glob": "^7.1.3",
    "typescript": "^3.4.3"
  },
  "precommit": [
    "yarn format",
    "yarn lint"
  ],
  "devDependencies": {
    "@types/faker": "^4.1.5",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.5",
    "jest": "^24.7.1",
    "precommit": "^1.2.2",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1"
  }
}
